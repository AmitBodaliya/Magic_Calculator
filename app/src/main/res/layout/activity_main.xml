                    android:layout_height="@dimen/cardHeight"
                    android:layout_marginHorizontal="@dimen/cardMarginHor"
                    android:layout_marginVertical="@dimen/cardMarginVer"
                    app:cardBackgroundColor="@color/app_theme_color_2"
                    app:cardCornerRadius="@dimen/cardCornerRadius"
