<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:autofillHints=""
        android:background="@null"

        android:scrollbars="none"

        android:cursorVisible="true"
        android:inputType="text"
        android:textCursorDrawable="@color/colorAccent"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayoutPAD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35" />

    <TextView
        android:id="@+id/result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:textColor="@color/green"

        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayoutPAD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText" />



    <LinearLayout
        android:id="@+id/linearLayoutPAD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:orientation="horizontal">

            <androidx.cardview.widget.CardView
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:cardCornerRadius="25dp"
                app:cardElevation="0dp"
                android:layout_marginStart="37dp"
                android:layout_gravity="center_vertical"
                app:cardBackgroundColor="@android:color/transparent"
                >

                <ImageButton
                    android:id="@+id/trickButton1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="10dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/clock"
                    android:background="?attr/selectableItemBackground"
                    android:contentDescription="@string/todo"
                    />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:cardCornerRadius="25dp"
                app:cardElevation="0dp"
                android:layout_marginStart="20dp"
                android:layout_gravity="center_vertical"
                app:cardBackgroundColor="@android:color/transparent"
                >

                <ImageButton
                    android:id="@+id/trickButton2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15dp"
                    android:src="@drawable/btn_unitconverter"
                    android:background="?attr/selectableItemBackground"
                    android:contentDescription="@string/todo"
                    />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="45dp"
                android:layout_height="45dp"
                app:cardCornerRadius="25dp"
                app:cardElevation="0dp"
                android:layout_marginStart="20dp"
                android:layout_gravity="center_vertical"
                app:cardBackgroundColor="@android:color/transparent"
                >

                <ImageButton
                    android:id="@+id/trickButton3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:padding="15dp"
                    android:src="@drawable/btn_scientific"
                    android:background="?attr/selectableItemBackground"
                    android:contentDescription="@string/todo"
                    />

            </androidx.cardview.widget.CardView>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="end"
                android:orientation="horizontal">


                <androidx.cardview.widget.CardView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    app:cardCornerRadius="25dp"
                    app:cardElevation="0dp"
                    android:layout_marginEnd="37dp"
                    android:layout_gravity="center_vertical"
                    app:cardBackgroundColor="@android:color/transparent"
                    >

                    <ImageButton
                        android:id="@+id/imageButtonBack"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="15dp"
                        android:src="@drawable/ic_baseline_backspace_24"
                        android:background="?attr/selectableItemBackground"
                        android:contentDescription="@string/todo"
                        />


                </androidx.cardview.widget.CardView>

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="30sp"
            android:layout_marginHorizontal="20dp"
            android:orientation="horizontal">

            <View
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="?android:attr/listDivider" />

        </LinearLayout>

        <LinearLayout
